{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore": "Warning"
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft.AspNetCore": "Warning",
        "Microsoft.EntityFrameworkCore": "Warning"
      }
    },
    "WriteTo": [
      { "Name": "Console" }
    ]
  },
  "ConnectionStrings": {
    "BenchmarkDb": "Data Source=benchmark.db"
  },
  "Ollama": {
    "BaseUrl": "http://localhost:11434",
    "DefaultTimeoutSeconds": 120,
    "DefaultTemperature": 0.7,
    "MaxRetries": 3
  },
  "OpenAi": {
    "ApiKey": "",
    "OrganizationId": "",
    "ProjectId": "",
    "DefaultTimeoutSeconds": 120,
    "MaxRetries": 3
  },
  "AllowedHosts": "*"
}
